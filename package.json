{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "nightwatch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "geckodriver": "^1.16.2",
    "nightwatch": "^1.1.10",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "aws-sdk": "^2.431.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.3",
    "file-type": "^10.9.0",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "multiparty": "^4.2.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-bootstrap": "^1.0.0-beta.6",
    "react-event-calendar": "^0.3.0",
    "validator": "^10.11.0"
  }
}
